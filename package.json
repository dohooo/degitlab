{
  "name": "degitlab",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@7.2.1",
  "description": "",
  "author": "Dohooo <zhaodonghao586@outlook.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/dohooo",
  "homepage": "https://github.com/dohooo/degitlab#readme",
  "bin": {
    "degitlab": "./dist/index.mjs"
  },
	"engines": {
		"node": ">=16",
		"pnpm": ">=7",
		"npm": "please-use-pnpm",
		"yarn": "please-use-pnpm"
	},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dohooo/degitlab.git"
  },
  "bugs": "https://github.com/dohooo/degitlab/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "build:watch": "npx nodemon --exec 'pnpm run build' -w src -e 'ts'",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/ni": "^0.16.2",
    "@antfu/utils": "^0.5.2",
    "@dohooo/eslint-config": "*",
    "@types/node": "^17.0.44",
    "bumpp": "^8.0.0",
    "eslint": "^8.17.0",
    "esno": "^0.16.3",
    "nodemon": "^2.0.16",
    "pnpm": "^7.2.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.3",
    "unbuild": "^0.7.4",
    "vite": "^2.9.12",
    "vitest": "^0.15.1"
  },
  "dependencies": {
    "meow": "^10.1.3"
  }
}
