    "@types/node": "^17.0.44",
    "@types/parse-git-config": "^3.0.1",
    "@types/tar": "^6.1.2",
    "bumpp": "^8.0.0",
    "eslint": "^8.17.0",
    "esno": "^0.16.3",
    "nodemon": "^2.0.16",
    "pnpm": "^7.2.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.3",
    "unbuild": "^0.7.4",
    "vite": "^2.9.12",
    "vitest": "^0.15.1"
  },
  "dependencies": {
    "@gitbeaker/node": "^35.7.0",
    "decompress": "^4.2.1",
    "fs-extra": "^10.1.0",
    "git-config-path": "^2.0.0",
    "home-or-tmp": "^4.0.0",
    "meow": "^10.1.3",
    "nodejs-file-downloader": "^4.10.1",
    "parse-git-config": "^3.0.0",
    "simple-git": "^3.11.0",
    "tar": "^6.1.11"
  }
